{
  "name": "slopstudios3",
  "version": "0.0.1",
  "description": "A modern AI-powered media and art platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist coverage .nyc_output",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "docs:api": "npx @redocly/cli build-docs docs/api/openapi.yaml -o docs/api/index.html",
    "docs:serve": "npx @redocly/cli@1.25.0 preview-docs docs/api/openapi.yaml",
    "docs:lint": "npx @redocly/cli lint docs/api/openapi.yaml",
    "dev:debug": "NODE_OPTIONS='--inspect' tsx watch src/index.ts",
    "db:migrate": "echo 'Database migrations not yet configured'",
    "db:seed": "echo 'Database seeding not yet configured'",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:build": "docker build -t slopstudios3 .",
    "docker:run": "docker run -p 3000:3000 slopstudios3",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slopstudios/slopstudios3.git"
  },
  "keywords": [
    "ai",
    "media",
    "art",
    "platform"
  ],
  "author": "Slop Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slopstudios/slopstudios3/issues"
  },
  "homepage": "https://github.com/slopstudios/slopstudios3#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/generic-pool": "^3.1.11",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "socket.io-client": "^4.8.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.52.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/redis": "^7.0.2",
    "@fastify/session": "^11.1.1",
    "@fastify/static": "^9.0.0",
    "@sinclair/typebox": "^0.34.15",
    "@strudel/core": "^1.2.5",
    "@strudel/mini": "^1.2.5",
    "@strudel/transpiler": "^1.2.5",
    "@strudel/webaudio": "^1.2.6",
    "@types/bcrypt": "^6.0.0",
    "acorn": "^8.15.0",
    "bcrypt": "^6.0.0",
    "connect-redis": "^9.0.0",
    "dotenv": "^16.4.7",
    "escodegen": "^2.1.0",
    "fastify": "^5.2.1",
    "generic-pool": "^3.9.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "socket.io": "^4.8.1",
    "superdough": "^1.2.5",
    "web-audio-engine": "^0.13.4"
  }
}
